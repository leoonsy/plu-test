{  
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "compilerOptions": {  
    "rootDir": "src",  
    "outDir": "dist",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["node"]
  },  
  "include": [  
    "src/**/*.ts",
    "src/**/*.vue",
    "webpack.config.js",
    "postcss.config.js"
  ],  
  "exclude": [  
    "node_modules",
    "dist/**/*"
  ]  
} 